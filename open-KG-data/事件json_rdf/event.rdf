                            <ns0:P1 rdf:resource="http://www.openkg.cn/COVID-19/全国累计治愈出院632例"/>
                            <ns0:P2 rdf:resource="http://www.openkg.cn/COVID-19/据国家卫健委，2月3日0—24时，新增治愈出院病例157例。截至2月3日24时，累计治愈出院病例632例。"/>
                            <ns0:P3 rdf:resource="https://m.weibo.cn/status/4468154966827377?"/>
                            <ns0:P4 rdf:resource="http://www.openkg.cn/COVID-19/人民网"/>
