  <rdf:Description rdf:about="http://www.openkg.cn/COVID-19/event/resource/R174">
    <rdf:type rdf:resource="http://www.openkg.cn/COVID-19/event/class/C3"/>
    <ns0:P10 rdf:resource="http://www.openkg.cn/COVID-19/人社厅明电[2020]5号"/>
